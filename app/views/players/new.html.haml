%section.content-header
  %h1 Players
%section.content
  .box.box-primary
    .box-header.with-border
      %h3.box-title New
    %form.form-horizontal{:action => players_path, :enctype =>"multipart/form-data", :method => "post"}
      = render :partial => 'players/form'
      .box-footer
        %button.btn.btn-primary.pull-right{:type => 'submit'} Create
